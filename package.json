{
	"name": "lwc-typings-generator",
	"version": "3.1.0",
	"description": "LWC typings generation",
	"author": "Ziemniakoss",
	"bugs": "https://github.com/Ziemniakoss/lwc-typings-generator/issues",
	"dependencies": {
		"@salesforce/command": "^4.2.2",
		"chokidar": "^3.5.3",
		"tslib": "^2.3.1",
		"web-tree-sitter": "^0.20.5"
	},
	"devDependencies": {
		"@oclif/plugin-help": "^3",
		"@oclif/test": "^2.1.0",
		"@salesforce/dev-scripts": "^2.0.1",
		"@salesforce/ts-sinon": "^1.3.21",
		"jsforce": "^1.11.0",
		"mocha": "^9.2.2",
		"nyc": "^15",
		"prettier": "^2.6.2",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"files": [
		"/lib",
		"/messages",
		"/resources",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/Ziemniakoss/sfdx-metadata-splitter",
	"keywords": [
		"sfdx-plugin",
		"typings",
		"lwc"
	],
	"license": "MIT",
	"oclif": {
		"commands": "./lib/commands",
		"bin": "sfdx",
		"topics": {},
		"devPlugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "Ziemniakoss/lwc-typings-generator",
	"scripts": {
		"build": "tsc -p .",
		"postpack": "rm -f oclif.manifest.json",
		"test": "nyc --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\" --timeout 10000",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"version": "oclif-dev readme && git add README.md"
	}
}
